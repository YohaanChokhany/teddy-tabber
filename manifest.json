{
  "manifest_version": 3,
  "name": "Teddy Tabber",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": ["https://*/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
